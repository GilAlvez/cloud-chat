{
    "env": {
        "es2022": true,
        "node": true
    },
    "extends": [
      "standard-with-typescript" ,
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "@typescript-eslint/consistent-type-definitions": "off"
    }
}
